{
  "parser": "babel-eslint",
  "root": true,
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "ecmaFeatures": {
    "arrowFunctions": true,
    "defaultParams": true,
    "destructuring": true,
    "modules": true,
    "jsx": true,
    "spread": true,
    "restParams": true,
    "templateStrings": true,
    "classes": true,
    "objectLiteralComputedProperties": true,
    "objectLiteralShorthandMethods": true,
    "objectLiteralShorthandProperties": true,
    "blockBindings": true,
  },
  "globals": {
    "inject": false,
    "$": false,
    "browser": false,
    "$$": false,
    "protractor": false,
    "element": false,
    "by": false,
    "phantom": false
  },
  "env": {
    "browser": true,
    "jasmine": true,
    "node": true
  },
  "rules": {
    "one-var": [1, "never"],
    "no-var": 2,
    "object-shorthand": 1,
    "arrow-spacing": 2,
    "arrow-parens": 2,
    "prefer-const": 1,
    "prefer-template": 1,
    "no-useless-concat": 1,
    "block-scoped-var": 1,
    "comma-dangle": [2, "always-multiline"],
    "quotes": [1, "single"],
    "curly": [2, "multi-line"],
    "semi": [2, "always"],
    "eqeqeq": [2, "allow-null"],
    "no-underscore-dangle": 0,
    "consistent-return": 0,
    "no-unused-vars": [1, {"vars": "all", "args": "after-used"}],
    "camelcase": [2, {properties: "never"}],
    "new-cap": [2, {
      capIsNewExceptions: [
        "Set",
        "NumberFormat",
        "MakePublicApi"
      ]
    }],
    "no-multiple-empty-lines": [2, { max: 2 }],
    "key-spacing": 0,
    "no-shadow": 1,
    "no-console": 0,
    "no-unused-expressions": 0,
    "no-process-exit": 0,
    "babel/object-curly-spacing": 0,
    "babel/arrow-parens": 2,
  },
  "plugins": [
    "babel",
    "react",
  ]
}
